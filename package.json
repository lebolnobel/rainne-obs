{
  "name": "rainnes-obs",
  "description": "Application de comptage pour l'opération Sauvetage des batraciens, organisé par Natagora",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "npm run check && vite build",
    "check": "npm run lint && npm run tsc",
    "dev": "vite --host",
    "lint": "eslint . --ext ts,tsx --max-warnings 0",
    "preview": "vite preview --host --port 8080",
    "preview-dev": "vite build --watch & vite preview --host --port 8080",
    "prepare": "husky",
    "reset": "rm -rf node_modules/ && rm package-lock.json && npm i && npm audit fix",
    "sitemap": "node tools/sitemap.js",
    "start": "vite",
    "tsc": "npx tsc",
    "upgrade": "npm update --save && npm update --save-dev",
    "watch-css": "npx tailwindcss -i ./src/styles/input.css -o ./src/styles/styles.css --watch"
  },
  "dependencies": {
    "@netlify/functions": "^2.8.2",
    "@supabase/supabase-js": "^2.86.2",
    "braces": "^3.0.3",
    "fuse.js": "^7.1.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-icons": "^5.5.0",
    "react-lazy-load-image-component": "^1.6.3",
    "react-router-dom": "^6.30.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^22.19.1",
    "@types/react": "19.2.9",
    "@types/react-dom": "19.2.3",
    "@types/react-lazy-load-image-component": "^1.6.5",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.22",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.24",
    "husky": "^9.1.7",
    "network-information-types": "^0.1.1",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "sharp": "^0.33.5",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.9.3",
    "vite": "^6.4.1"
  }
}
